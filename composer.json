{
    "name": "becklyn/schema-org",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "becklyn/rad-bundles": "^1.1.0",
        "symfony/config": "^5.4.3 || ^6.0.3",
        "symfony/dependency-injection": "^5.4.3 || ^6.0.3",
        "symfony/http-kernel": "^5.4.3 || ^6.0.3",
        "twig/twig": "^3.3.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.1.5"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\SchemaOrg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\SchemaOrg\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
