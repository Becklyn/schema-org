{
    "name": "becklyn/schema-org",
    "type": "symfony-bundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "becklyn/rad-bundles": "^1.0",
        "symfony/config": "^4.4.13 || ^5.1.5",
        "symfony/dependency-injection": "^4.4.13 || ^5.1.5",
        "symfony/http-kernel": "^4.4.13 || ^5.1.5",
        "symfony/phpunit-bridge": "^5.1.5",
        "twig/twig": "^2.11 || ^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\SchemaOrg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\SchemaOrg\\": "tests/"
        }
    }
}
